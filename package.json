  "main": "src/index.js",
  "dependencies": {
    "autoprefixer": "9.7.3",
    "date-fns": "2.8.1",
    "postcss-cli": "6.1.3",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-scripts": "3.3.0",
    "tailwindcss": "1.1.4"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "1.3.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.19.1"
  },
  "scripts": {
    "start": "npm-run-all -p start:css start:js",
    "build": "npm-run-all build:css build:js",
    "start:js": "react-scripts start",
