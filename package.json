    "start:css": "postcss src/css/tailwind.src.css -o src/tailwind.css -w",
    "build:css": "postcss src/css/tailwind.src.css -o src/tailwind.css --env production",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "browserslist": [
    ">0.2%",
